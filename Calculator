package Project;

public class Calculator {

public int add(int a, int b)
{
return(a+b);
}

public int sub(int c,int d)
{
return(c-d);
}

public int mul(int e,int f)
{
return(e*f);
}

public float div(float g,float h)
{
return(g/h);
}
}
