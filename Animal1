package anudip;

public class Animal1 {
	
	public static void main(String[] args) {
		
		 Animal animal = new Animal();
		 
		 Dog dog = new Dog();
		 Cat cat = new Cat();
		 
		 System.out.print("Animal sounds:");
		 animal.makeSound();
		 
		 System.out.print("\nDog sounds:");
		 dog.makeSound();
		 
		 System.out.print("\nCat sounds:");
		 cat.makeSound();
		 
		 }

}
